#include <iostream>
using namespace std;

int main() {
    int valor;
    cout << "Ingrese un valor: ";
    cin >> valor;

    if (valor >= 100) {
        cout << "Tiene 3 dígitos" << endl;
    } else if (valor >= 10) {
        cout << "Tiene 2 dígitos" << endl;
    } else if (valor >= 0) {
        cout << "Tiene 1 dígito" << endl;
    } else {
        cout << "Falso" << endl;
    }

    return 0;
}
