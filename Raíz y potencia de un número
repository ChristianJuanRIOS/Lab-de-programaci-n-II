#include <iostream>
#include <cmath>

using namespace std;

int main() {
    double numero;
    
    cout << "Ingrese un numero: ";
    cin >> numero;
    
    double raizCuadrada = sqrt(numero);
    cout << "La raiz cuadrada de " << numero << " es: " << raizCuadrada << endl;
    
    double cuadrado = pow(numero, 2);
    cout << "La potencia de " << numero << " es: " << cuadrado << endl;
    
    return 0;
}
