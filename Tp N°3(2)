#include <iostream>
using namespace std;

double calcularMayor(double num1, double num2) {
    if (num1 > num2) {
        return num1;
    } else {
        return num2;
    }
}

int main() {
    double num1, num2;
    cout << "Ingrese dos nÃºmeros: ";
    cin >> num1 >> num2;
    cout << "El mayor es: " << calcularMayor(num1, num2) << endl;
    return 0;
}
