#include <iostream>
#include <vector>

using namespace std;

void ordenamientoInsercion(vector<int> & arr) {
    int n = arr.size();
    for (int i = 1; i < n; ++i) {
        int key = arr[i];
        int j = i - 1;

        while (j >= 0 && arr[j] > key) {
            arr[j + 1] = arr[j];
            j = j - 1;
        }
        arr[j + 1] = key;
    }
}

int main() {
    int n;
    cout << "Ingrese el tamaÃ±o del vector: ";
    cin >> n;

    vector<int> arr(n);
    cout << "Ingrese los elementos del vector: ";
    for (int i = 0; i < n; ++i) {
        cin >> arr[i];
    }

    ordenamientoInsercion(arr);

    cout << "El vector ordenado es: ";
    for (int i = 0; i < n; ++i) {
        cout << arr[i] << " ";
    }
    cout << endl;

    return 0;
}
