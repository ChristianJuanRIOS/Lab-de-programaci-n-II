#include <iostream>
using namespace std;

void mostrarTablaMultiplicar(int num) {
    cout << "Tabla de multiplicar del " << num << ":" << endl;
    for (int i = 1; i <= 10; i++) {
        cout << num << " X " << i << " = " << (num * i) << endl;
    }
}

int main() {
    int num;
    cout << "Ingrese un nÃºmero para mostrar su tabla de multiplicar: ";
    cin >> num;
    mostrarTablaMultiplicar(num);
    return 0;
}
