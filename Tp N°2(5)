#include <iostream>
using namespace std;

int main() {
    int numero;

    cout << "Ingresa un número del 1 al 10: ";
    cin >> numero;

    if (numero < 1 || numero > 10) {
        cout << "Número fuera de rango. Por favor, ingresa un número del 1 al 10." << endl;
        return 1;
    }

    cout << "Tabla de multiplicar del " << numero << ":" << endl;
    for (int i = 1; i <= 12; i++) {
        cout << numero << " x " << i << " = " << numero * i << endl;
    }

    return 0;
}
