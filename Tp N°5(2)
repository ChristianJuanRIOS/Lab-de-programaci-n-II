#include <iostream>
#include <vector>
#include <algorithm>

using namespace std;

int main() {
    int n;
    cout << "Ingrese el tamaÃ±o del vector: ";
    cin >> n;

    vector<int> vec(n);

    cout << "Ingrese los elementos del vector: ";
    for (int i = 0; i < n; ++i) {
        cin >> vec[i];
    }

    sort(vec.begin(), vec.end());

    cout << "El vector ordenado es: ";
    for (int i = 0; i < n; ++i) {
        cout << vec[i] << " ";
    }
    cout << endl;

    return 0;
}
