#include <iostream>
#include <vector>

using namespace std;

void swap(int &a, int &b) {
    int temp = a;
    a = b;
    b = temp;
}

void ordenamientoBurbuja(vector<int> &arr) {
    int n = arr.size();
    for (int i = 0; i < n-1; ++i) {
        for (int j = 0; j < n-i-1; ++j) {
            if (arr[j] > arr[j+1]) {
                swap(arr[j], arr[j+1]);
            }
        }
    }
}

int main() {
    int n;
    cout << "Ingrese el tamaño del vector: ";
    cin >> n;
    
    vector<int> vec(n);
    cout << "Ingrese los " << n << " elementos del vector: ";
    for (int i = 0; i < n; ++i) {
        cin >> vec[i];
    }
    
    cout << "Vector antes de ordenar: ";
    for (int i = 0; i < n; ++i) {
        cout << vec[i] << " ";
    }
    cout << endl;
    
    ordenamientoBurbuja(vec);
    
    cout << "Vector después de ordenar: ";
    for (int i = 0; i < n; ++i) {
        cout << vec[i] << " ";
    }
    cout << endl;
    
    return 0;
}
